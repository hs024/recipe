{
    "version": 2,
    "builds": [
      {
        "src": "website/wsgi.py",
        "use": "@vercel/python",
        "config":{
            "maxLambdaSize":"15mb","runtime":"python"
        }
      },
      {
       
        "src": "build_files.sh",
        "use": "@vercel/static-build" ,
        "config":{
            "distDir":"staticfiles_build"
        }
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/ststic/$1"
          },
      {
        "src": "/(.*)",
        "dest": "website/wsgi.py"
      }
    ]
  }
  